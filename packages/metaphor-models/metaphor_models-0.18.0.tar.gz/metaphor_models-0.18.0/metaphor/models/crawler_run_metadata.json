{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "RunStatus": {
            "enum": [
                "FAILURE",
                "PENDING",
                "SUCCESS"
            ],
            "type": "string"
        }
    },
    "description": "Metadata of a crawler run",
    "properties": {
        "crawlerName": {
            "default": "",
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "endTime": {
            "format": "date-time",
            "type": "string"
        },
        "entityCount": {
            "default": 0,
            "type": "number"
        },
        "errorMessage": {
            "type": "string"
        },
        "logFile": {
            "type": "string"
        },
        "platform": {
            "enum": [
                "AIRFLOW",
                "BIGQUERY",
                "DBT_METRIC",
                "DBT_MODEL",
                "DOCUMENTDB",
                "DYNAMODB",
                "ELASTICSEARCH",
                "EXTERNAL",
                "FIVETRAN",
                "GLUE",
                "LOOKER",
                "LOOKER_EXPLORE",
                "LOOKER_VIEW",
                "METABASE",
                "MSSQL",
                "MYSQL",
                "POSTGRESQL",
                "POWER_BI",
                "POWER_BI_DATASET",
                "RDS",
                "REDIS",
                "REDSHIFT",
                "S3",
                "SNOWFLAKE",
                "SYNAPSE",
                "TABLEAU",
                "TABLEAU_DATASOURCE",
                "THOUGHT_SPOT",
                "THOUGHT_SPOT_DATA_OBJECT",
                "UNITY_CATALOG",
                "UNKNOWN"
            ],
            "type": "string"
        },
        "stackTrace": {
            "type": "string"
        },
        "startTime": {
            "format": "date-time",
            "type": "string"
        },
        "status": {
            "$ref": "#/definitions/RunStatus"
        }
    },
    "type": "object"
}

